               placeholder={\"*********\"}\r\n                                    {...field}\r\n                                    type={\"password\"}\r\n                                />\r\n                              </FormControl>\r\n                              <FormMessage/>\r\n\r\n                            </FormItem>\r\n                        )}\r\n                    />\r\n                    <FormField\r\n                        name={\"confirmPassword\"}\r\n                        render={({field}) => (\r\n                            <FormItem>\r\n                              <FormLabel>Confirm Password</FormLabel>\r\n                              <FormControl>\r\n                                <Input\r\n                                    placeholder={\"*********\"}\r\n                                    {...field}\r\n                                    type={\"password\"}\r\n                                />\r\n                              </FormControl>\r\n                              <FormMessage/>\r\n\r\n                            </FormItem>\r\n                        )}\r\n                    />\r\n\r\n                    <Button type={\"submit\"}\r\n                            disabled={isPending}\r\n                            className={\"w-full\"}>\r\n                      Signup\r\n                    </Button>\r\n                  </div>\r\n\r\n                  <div className={\"text-center text-sm\"}>\r\n                    Already have an account?{\" \"}\r\n                    <Link href={\"/login\"} className={\"text-blue-500 underline\"}>\r\n                      Login\r\n                    </Link>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </Form>\r\n          </CardContent>\r\n        </Card>\r\n      </div>\r\n  )\r\n}\r\n
